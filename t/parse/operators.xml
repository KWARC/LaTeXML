  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Integration and Summation</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="\sum_{a=0}^{\infty}f_{a}(c^{a})" text="((sum _ (a = 0)) ^ infinity)@((f _ a)@(c ^ a))">
          <XMath>
            <XMApp>
              <XMApp scriptpos="mid">
                <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                  <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                  <XMApp>
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                    <XMTok meaning="0" role="NUMBER">0</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
              </XMApp>
              <XMDual>
                <XMApp>
                  <XMRef idref="S1.Ex1.m1.1"/>
                  <XMRef idref="S1.Ex1.m1.2"/>
                </XMApp>
                <XMApp>
                  <XMApp xml:id="S1.Ex1.m1.1">
                    <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                    <XMTok role="FUNCTION" font="italic">f</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                  </XMApp>
                  <XMWrap>
                    <XMTok role="OPEN" stretchy="false">(</XMTok>
                    <XMApp xml:id="S1.Ex1.m1.2">
                      <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                      <XMTok role="ID" font="italic">c</XMTok>
                      <XMTok role="ID" font="italic">a</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">)</XMTok>
                  </XMWrap>
                </XMApp>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="\sum_{a=0}^{\infty}\sum_{b=-a}^{a}f_{a}(c^{a})g_{b}(c^{b})" text="((sum _ (a = 0)) ^ infinity)@(((sum _ (b = - a)) ^ a)@((f _ a)@(c ^ a) * (g _ b)@(c ^ b)))">
          <XMath>
            <XMApp>
              <XMApp scriptpos="mid">
                <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                  <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                  <XMApp>
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                    <XMTok meaning="0" role="NUMBER">0</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
              </XMApp>
              <XMApp>
                <XMApp scriptpos="mid">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                  <XMApp scriptpos="mid">
                    <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                    <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                    <XMApp>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                      <XMTok role="ID" font="italic">b</XMTok>
                      <XMApp>
                        <XMTok meaning="minus" role="ADDOP">-</XMTok>
                        <XMTok role="ID" font="italic">a</XMTok>
                      </XMApp>
                    </XMApp>
                  </XMApp>
                  <XMTok role="ID" font="italic">a</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMRef idref="S1.Ex2.m1.1"/>
                      <XMRef idref="S1.Ex2.m1.2"/>
                    </XMApp>
                    <XMApp>
                      <XMApp xml:id="S1.Ex2.m1.1">
                        <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                        <XMTok role="FUNCTION" font="italic">f</XMTok>
                        <XMTok role="ID" font="italic">a</XMTok>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">(</XMTok>
                        <XMApp xml:id="S1.Ex2.m1.2">
                          <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                          <XMTok role="ID" font="italic">c</XMTok>
                          <XMTok role="ID" font="italic">a</XMTok>
                        </XMApp>
                        <XMTok role="CLOSE" stretchy="false">)</XMTok>
                      </XMWrap>
                    </XMApp>
                  </XMDual>
                  <XMDual>
                    <XMApp>
                      <XMRef idref="S1.Ex2.m1.3"/>
                      <XMRef idref="S1.Ex2.m1.4"/>
                    </XMApp>
                    <XMApp>
                      <XMApp xml:id="S1.Ex2.m1.3">
                        <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                        <XMTok role="FUNCTION" font="italic">g</XMTok>
                        <XMTok role="ID" font="italic">b</XMTok>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">(</XMTok>
                        <XMApp xml:id="S1.Ex2.m1.4">
                          <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                          <XMTok role="ID" font="italic">c</XMTok>
                          <XMTok role="ID" font="italic">b</XMTok>
                        </XMApp>
                        <XMTok role="CLOSE" stretchy="false">)</XMTok>
                      </XMWrap>
                    </XMApp>
                  </XMDual>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" xml:id="S1.Ex3.m1" tex="\sum_{a=0}^{\infty}\sum_{b=-a}^{a}\lim_{c\rightarrow 0}f_{a}(c^{a})g_{b}(c^{b})" text="((sum _ (a = 0)) ^ infinity)@(((sum _ (b = - a)) ^ a)@((limit _ (c rightarrow 0))@((f _ a)@(c ^ a) * (g _ b)@(c ^ b))))">
          <XMath>
            <XMApp>
              <XMApp scriptpos="mid">
                <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                  <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                  <XMApp>
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok role="ID" font="italic">a</XMTok>
                    <XMTok meaning="0" role="NUMBER">0</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
              </XMApp>
              <XMApp>
                <XMApp scriptpos="mid">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                  <XMApp scriptpos="mid">
                    <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                    <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                    <XMApp>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                      <XMTok role="ID" font="italic">b</XMTok>
                      <XMApp>
                        <XMTok meaning="minus" role="ADDOP">-</XMTok>
                        <XMTok role="ID" font="italic">a</XMTok>
                      </XMApp>
                    </XMApp>
                  </XMApp>
                  <XMTok role="ID" font="italic">a</XMTok>
                </XMApp>
                <XMApp>
                  <XMApp scriptpos="mid">
                    <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                    <XMTok meaning="limit" role="LIMITOP" scriptpos="mid">lim</XMTok>
                    <XMApp>
                      <XMTok name="rightarrow" role="ARROW">→</XMTok>
                      <XMTok role="ID" font="italic">c</XMTok>
                      <XMTok meaning="0" role="NUMBER">0</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMRef idref="S1.Ex3.m1.1"/>
                        <XMRef idref="S1.Ex3.m1.2"/>
                      </XMApp>
                      <XMApp>
                        <XMApp xml:id="S1.Ex3.m1.1">
                          <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                          <XMTok role="FUNCTION" font="italic">f</XMTok>
                          <XMTok role="ID" font="italic">a</XMTok>
                        </XMApp>
                        <XMWrap>
                          <XMTok role="OPEN" stretchy="false">(</XMTok>
                          <XMApp xml:id="S1.Ex3.m1.2">
                            <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                            <XMTok role="ID" font="italic">c</XMTok>
                            <XMTok role="ID" font="italic">a</XMTok>
                          </XMApp>
                          <XMTok role="CLOSE" stretchy="false">)</XMTok>
                        </XMWrap>
                      </XMApp>
                    </XMDual>
                    <XMDual>
                      <XMApp>
                        <XMRef idref="S1.Ex3.m1.3"/>
                        <XMRef idref="S1.Ex3.m1.4"/>
                      </XMApp>
                      <XMApp>
                        <XMApp xml:id="S1.Ex3.m1.3">
                          <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                          <XMTok role="FUNCTION" font="italic">g</XMTok>
                          <XMTok role="ID" font="italic">b</XMTok>
                        </XMApp>
                        <XMWrap>
                          <XMTok role="OPEN" stretchy="false">(</XMTok>
                          <XMApp xml:id="S1.Ex3.m1.4">
                            <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                            <XMTok role="ID" font="italic">c</XMTok>
                            <XMTok role="ID" font="italic">b</XMTok>
                          </XMApp>
                          <XMTok role="CLOSE" stretchy="false">)</XMTok>
                        </XMWrap>
                      </XMApp>
                    </XMDual>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Operators</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex4">
        <Math mode="display" xml:id="S2.Ex4.m1" tex="Da" text="D@(a)">
          <XMath>
            <XMApp>
              <XMTok role="OPERATOR" font="italic">D</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex5">
        <Math mode="display" xml:id="S2.Ex5.m1" tex="Df" text="D@(f)">
          <XMath>
            <XMApp>
              <XMTok role="OPERATOR" font="italic">D</XMTok>
              <XMTok role="FUNCTION" font="italic">f</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" xml:id="S2.Ex6.m1" tex="Df(a)" text="(D@(f))@(a)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S2.Ex6.m1.2"/>
                <XMRef idref="S2.Ex6.m1.1"/>
              </XMApp>
              <XMApp>
                <XMApp xml:id="S2.Ex6.m1.2">
                  <XMTok role="OPERATOR" font="italic">D</XMTok>
                  <XMTok role="FUNCTION" font="italic">f</XMTok>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex6.m1.1" font="italic">a</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex7">
        <Math mode="display" xml:id="S2.Ex7.m1" tex="DGHf" text="D@(G@(H@(f)))">
          <XMath>
            <XMApp>
              <XMTok role="OPERATOR" font="italic">D</XMTok>
              <XMApp>
                <XMTok role="OPERATOR" font="italic">G</XMTok>
                <XMApp>
                  <XMTok role="OPERATOR" font="italic">H</XMTok>
                  <XMTok role="FUNCTION" font="italic">f</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex8">
        <Math mode="display" xml:id="S2.Ex8.m1" tex="DGHf(a)" text="(D@(G@(H@(f))))@(a)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S2.Ex8.m1.2"/>
                <XMRef idref="S2.Ex8.m1.1"/>
              </XMApp>
              <XMApp>
                <XMApp xml:id="S2.Ex8.m1.2">
                  <XMTok role="OPERATOR" font="italic">D</XMTok>
                  <XMApp>
                    <XMTok role="OPERATOR" font="italic">G</XMTok>
                    <XMApp>
                      <XMTok role="OPERATOR" font="italic">H</XMTok>
                      <XMTok role="FUNCTION" font="italic">f</XMTok>
                    </XMApp>
                  </XMApp>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex8.m1.1" font="italic">a</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" xml:id="S2.Ex9.m1" tex="D(a)(b)" text="(D@(a))@(b)">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S2.Ex9.m1.4"/>
                <XMRef idref="S2.Ex9.m1.3"/>
              </XMApp>
              <XMApp>
                <XMDual xml:id="S2.Ex9.m1.4">
                  <XMApp>
                    <XMRef idref="S2.Ex9.m1.1"/>
                    <XMRef idref="S2.Ex9.m1.2"/>
                  </XMApp>
                  <XMApp>
                    <XMTok role="OPERATOR" xml:id="S2.Ex9.m1.1" font="italic">D</XMTok>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMTok role="ID" xml:id="S2.Ex9.m1.2" font="italic">a</XMTok>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMApp>
                </XMDual>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex9.m1.3" font="italic">b</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
